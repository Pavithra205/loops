#calculates the sum of even numbers from 1 to 10 using a while loop.
public class s {
    public static void main(String[] args) {
        int a = 0;
        int b = 1;

        while (b <= 10) {
            if (b % 2 == 0) {
                a +=b;
            }
            b++;
        }

        System.out.println("The sum of even numbers from 1 to 10 is: " + a);
    }
}
